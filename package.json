{
  "name": "pagenodes",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:worker && vite build",
    "build:worker": "vite build --config vite.worker.config.js",
    "build:server": "vite build --config vite.server.config.js",
    "lint": "eslint .",
